#include <iostream>

using namespace std;

int main() {
    int num1, num2, num3, num4, num5, num6, num7;

    cout << "Введіть семь цілих чисел: ";
    cin >> num1 >> num2 >> num3 >> num4 >> num5 >> num6 >> num7;

    int max_num = num1;

    if (num2 > max_num) {
        max_num = num2;
    }
    if (num3 > max_num) {
        max_num = num3;
    }
    if (num4 > max_num) {
        max_num = num4;
    }
    if (num5 > max_num) {
        max_num = num5;
    }
    if (num6 > max_num) {
        max_num = num6;
    }
    if (num7 > max_num) {
        max_num = num7;
    }

    cout << "Максимальне число: " << max_num << endl;

    return 0;
}
