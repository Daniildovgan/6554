#include <iostream>

using namespace std;

int main() {
    int number1, number2;
    cout << "Введіть перше число: ";
    cin >> number1;
    cout << "Введіть друге число: ";
    cin >> number2;

    if (number1 < number2) {
        cout << "Менше число: " << number1 << endl;
    } else {
        cout << "Менше число: " << number2 << endl;
    }

    return 0;
}
